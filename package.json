{
  "name": "pokemon-deck-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}' '*.{js,ts,json,html}'",
