{
  "name": "pokemon-deck-viewer",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint --type-aware .",
    "check": "oxlint -—type-aware -—type-check",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "preview": "vite preview",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@types/node": "^25.3.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "latest",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.27",
    "lefthook": "latest",
    "oxfmt": "latest",
    "oxlint": "latest",
    "oxlint-tsgolint": "latest",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^24.14.0",
      "onFail": "download"
    }
  },
  "engines": {
    "node": "^24.14.0"
  },
  "packageManager": "pnpm@10.30.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "lefthook"
    ],
    "overrides": {}
  }
}
